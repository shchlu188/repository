<?xml version="1.0" encoding="UTF-8" ?>
<!--数据库连接-->
<!--映射配置文件的路径-->
<orm-factory>
    <property name="connection.url">jdbc:mysql://localhost:3306/mybatis?useSSL=false</property>
    <property name="connection.driverClass">com.mysql.jdbc.Driver</property>
    <property name="connection.username">root</property>
    <property name="connection.password">root</property>
    <mapping resource="mapper/User.mapper.xml"></mapping>
    <entity package="com.scl.domain"></entity>
</orm-factory>